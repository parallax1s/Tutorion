{
  "version": 2,
  "functions": {
    "api/mcp.js": {
      "includeFiles": "public/tutor-widget.html"
    }
  },
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "src": "^/.well-known/(.*)$", "dest": "/.well-known/$1" },
    { "src": "^/$", "dest": "/public/tutor-widget.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
